@model BeyondThemes.BeyondAdmin.Models.ShippingType

@{
    ViewBag.Title = "Delete Shipping Type";
    ViewBag.Description = "Shipping List";
    Layout = "~/Views/Shared/_Default.cshtml";
}

<h3>Are you sure you want to delete @Model.ShippingName?</h3>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-actions">
        <input type="submit" value="Delete" class="btn btn-danger"  /> | 
        @Html.ActionLink("No","Index","Shipping", null, new { @class = "btn btn-primary" })
    </div>
}

